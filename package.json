{
  "name": "genrizz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite --port 5174",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest --coverage",
    "lighthouse": "lighthouse-ci",
    "audit": "npm audit --fix --prod && npx snyk test",
    "docker:dev": "docker-compose -f docker-compose.yml up app-dev",
    "docker:prod": "docker-compose -f docker-compose.yml up app-prod",
    "check:port": "node scripts/check-port.js",
    "dev:safe": "node scripts/find-available-port.js && PORT=$(node scripts/find-available-port.js) npm run dev",
    "dev:custom": "PORT=${APP_PORT:-5174} npm run dev",
    "dev:auto": "node scripts/find-available-port.js && PORT=$(node scripts/find-available-port.js) npm run dev",
    "qa:full": "npm run test && npm run test:e2e && npm run lighthouse",
    "qa:phase1": "npm run test:e2e tests/e2e/atomic-interactions.spec.ts",
    "qa:phase2": "npm run test:e2e tests/e2e/login.spec.ts",
    "qa:phase7": "npm run lighthouse",
    "postinstall": "if [ \"$VERCEL\" != \"1\" ]; then npx playwright install; fi",
    "content:export": "node scripts/export-questions-json.js",
    "content:normalize": "node scripts/normalize-questions-json.js",
    "content:validate": "node scripts/validate-questions.js",
    "content:refresh": "npm run content:export && npm run content:normalize && npm run content:validate",
    "content:check": "npm run content:refresh",
    "test:flags": "vitest tests/flags.test.js",
    "test:daily-drop": "vitest tests/DailyDropCard.test.jsx"
  },
